{
  "expo": {
    "name": "Arya",
    "slug": "arya",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/aryaco.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/firestore"
    ],
    "orientation": "portrait",
    "icon": "./assets/aryaco.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/aryaco.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aray",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/aryaco.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aray",
      "googleServicesFile": "./google-services.json"
    }
  }
}